{
    "name": "pivot-table-engine",
    "version": "0.7.7",
    "description": "Pivot Table with filtering and sorting (no UI)",
    "license": "MIT",
    "author": {
        "name": "Pavel Dymkov",
        "email": "dymkov86@gmail.com"
    },
    "keywords": [
        "pivot table",
        "pivot",
        "table",
        "sort",
        "filter"
    ],
    "files": [
        "*.js",
        "types/**/*.d.ts",
        "!webpack.config.js",
        "!test/*"
    ],
    "main": "index.js",
    "types": "types",
    "scripts": {
        "build": "webpack --env production",
        "clean": "rimraf types/*",
        "prepublishOnly": "npm run clean && npm run build && npm run test && npm version patch && git push",
        "start": "webpack -w",
        "test": "mocha --config=\"test/.mocharc.json\" test/**/*.spec.js"
    },
    "devDependencies": {
        "logical-not": "^1.0.7",
        "mocha": "^8.3.2",
        "rimraf": "^3.0.2",
        "ts-loader": "^8.1.0",
        "typescript": "^4.2.3",
        "webpack": "^5.28.0",
        "webpack-cli": "^4.6.0"
    }
}
